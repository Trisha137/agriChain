generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Retailers {
  id  String  @id @default(uuid())
  name  String
  phone String
  hash  String
  crop  String  
  quality String
  quantity  Int 
  message String
  created_at  DateTime  @default(now())
}
